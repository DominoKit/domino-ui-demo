    <build>
        <plugins>
            <plugin>
                <groupId>net.ltgt.gwt.maven</groupId>
                <artifactId>gwt-maven-plugin</artifactId>
                <configuration>
                    <extra>${extra.directory}/gwt/extra</extra>
                    <moduleName>org.dominokit.domino.ui.demo.AppClientModule</moduleName>
                    <moduleShortName>app</moduleShortName>
                    <compilerArgs>
                        <!--<compilerArg>-style</compilerArg>-->
                        <!--<compilerArg>PRETTY</compilerArg>-->
                        <compilerArg>-compileReport</compilerArg>
                        <compilerArg>-extra</compilerArg>
                        <compilerArg>${build.directory}/gwt/extra</compilerArg>
                    </compilerArgs>
                    <codeserverArgs>
                        <!--<codeserverArg>-bindAddress</codeserverArg>-->
                        <!--<codeserverArg>192.168.1.5</codeserverArg>-->
                        <codeserverArg>-src</codeserverArg>
                        <codeserverArg>/mnt/CODE/GIT/domino-kit/domino-ui/src/main/java</codeserverArg>
                        <codeserverArg>-src</codeserverArg>
                        <codeserverArg>/mnt/CODE/GIT/domino-kit/domino-ui/target/generated-sources/annotations</codeserverArg>
                    </codeserverArgs>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-deploy-plugin</artifactId>
                <version>2.8.2</version>
                <configuration>
                    <skip>true</skip>
                </configuration>
            </plugin>
        </plugins>
    </build>
